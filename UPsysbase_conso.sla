        <PAGEOBJECT XPOS="173.701417322835" YPOS="1897.16598425197" OwnPage="3" ItemID="707665391" PTYPE="4" WIDTH="479.055118110236" HEIGHT="14.1732283464564" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L479.055 0 L479.055 14.1732 L0 14.1732 L0 0 Z" copath="M0 0 L479.055 0 L479.055 14.1732 L0 14.1732 L0 0 Z" gXpos="173.701417322835" gYpos="2779.05543307087" gWidth="0" gHeight="0" PSTYLE="Intro chapitre" LAYER="1" NEXTITEM="-1" BACKITEM="-1">
