        <PAGEOBJECT XPOS="173.701417322835" YPOS="5385.03874015749" OwnPage="11" ItemID="652615775" PTYPE="4" WIDTH="479.055118110236" HEIGHT="690.236220472442" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="14.1732283464567" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L479.055 0 L479.055 690.236 L0 690.236 L0 0 Z" copath="M0 0 L479.055 0 L479.055 690.236 L0 690.236 L0 0 Z" gXpos="-421.574173228346" gYpos="975.591496062994" gWidth="0" gHeight="0" ALIGN="3" LAYER="1" NEXTITEM="636474911" BACKITEM="652332623"/>
        <PAGEOBJECT XPOS="737.795905511811" YPOS="5385.03874015749" OwnPage="12" ItemID="636474911" PTYPE="4" WIDTH="479.055118110236" HEIGHT="690.236220472442" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="14.1732283464567" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L479.055 0 L479.055 690.236 L0 690.236 L0 0 Z" copath="M0 0 L479.055 0 L479.055 690.236 L0 690.236 L0 0 Z" gXpos="142.52031496063" gYpos="975.591496062994" gWidth="0" gHeight="0" ALIGN="3" LAYER="1" NEXTITEM="723503983" BACKITEM="652615775"/>
        <PAGEOBJECT XPOS="737.795905511811" YPOS="8030.70708661418" OwnPage="18" ItemID="652613471" PTYPE="4" WIDTH="340.157480314961" HEIGHT="691.653543307092" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="14.1732283464567" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L340.157 0 L340.157 691.654 L0 691.654 L0 0 Z" copath="M0 0 L340.157 0 L340.157 691.654 L0 691.654 L0 0 Z" gXpos="142.52031496063" gYpos="2739.37039370079" gWidth="0" gHeight="0" ALIGN="3" LAYER="1" NEXTITEM="738358223" BACKITEM="687564351"/>
        <PAGEOBJECT XPOS="173.701417322835" YPOS="8112.91181102363" OwnPage="17" ItemID="636421407" PTYPE="4" WIDTH="124.724409448819" HEIGHT="608.031496062997" FRTYPE="0" CLIPEDIT="0" GRTYP="6" PWIDTH="1" PCOLOR="Yellow" SHADE="99" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="2.83464566929134" TEXTRA="2.83464566929134" BEXTRA="2.83464566929134" REXTRA="2.83464566929134" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L124.724 0 L124.724 608.031 L0 608.031 L0 0 Z" copath="M0 0 L124.724 0 L124.724 608.031 L0 608.031 L0 0 Z" gXpos="173.701417322835" gYpos="2821.57511811024" gWidth="0" gHeight="0" GRSTARTX="0" GRSTARTY="304.0157480315" GRENDX="124.724409448819" GRENDY="304.0157480315" GRFOCALX="0" GRFOCALY="0" GRSCALE="1" GRSKEW="0" GRExt="3" GRExtM="3" GRTYPM="1" GRSTARTXM="0" GRSTARTYM="0" GRENDXM="124.724409448819" GRENDYM="0" GRFOCALXM="0" GRFOCALYM="0" GRSCALEM="1" GRSKEWM="0" LAYER="1" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle LINESPMode="1" FONT="Linux Biolinum O Regular" FONTSIZE="8"/>
                <ITEXT CH="Dés"/>
                <para PARENT="Développement section"/>
                <ITEXT CH="Comme indiqué, le UP!system n'emploie que des D10. Toute mention de «"/>
                <nbspace/>
                <ITEXT CH="dés"/>
                <nbspace/>
                <ITEXT CH="» ne concerne donc que ce type de polyhèdre."/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Notation des jets"/>
                <para PARENT="Développement section"/>
                <ITEXT CH="Tous les jets du UP!System sont présentés de la manière suivante :"/>
                <para PARENT="Petit texte"/>
                <ITEXT CPARENT="Petit texte souligné" CH="XgY"/>
                <ITEXT CH="."/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Ce qui signifie tout simplement que l’on lance X dés et en garde Y."/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Par exemple, un personnage lançant 6 dés et en gardant 3 eﬀectuera un jet « à 6g3 »."/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Pour ce qui est de présenter les caractéristiques à utiliser lors d’un jet de compétence, on emploie la notation suivante :"/>
                <para PARENT="Petit texte"/>
                <ITEXT CPARENT="Petit texte souligné" CH="compétence/trait"/>
                <ITEXT CH="."/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Ce qui permet d’indiquer quel trait utiliser (pour déterminer le nombre de dés gardés). Le domaine n’est pas précisé car chaque compétence n’appartient qu’à un seul domaine, celui-ci est donc implicite contrairement au trait, qui peut varier suivant la situation. Par exemple, un jet de la compétence Survie, du domaine Nature, eﬀectué avec le trait Mental sera un jet de survie/mental (le domaine Nature est implicite)."/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Incompétence"/>
                <para PARENT="Développement section"/>
                <ITEXT CH="Un personnage n'ayant aucun rang dans une compétence effectue simplement un jet avec le domaine d'appartenance seulement, sans pénalité."/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Par exemple, si un orateur possédant 3 rangs dans le domaine Social se retrouve contraint d'effectuer un jet de la compétence Séduction, dans laquelle il ne possède aucun rang, il lance simplement 3 dés (ceux issus de son domaine), sans malus particulier : bien que n'étant pas un grand séducteur, son aisance sociale générale le rattrape."/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Fort potentiel"/>
                <para PARENT="Développement section"/>
                <ITEXT CH="Si un personnage est amené à garder davantage de dés sur un jet qu’il n’en lance, il peut intervertir les deux valeurs. "/>
                <para PARENT="Petit texte"/>
                <ITEXT CH="Ainsi, un jet à 2g4 (qui, dans les faits, est un jet à 2g2 car il n’est pas possible de garder des dés qui n’existent pas"/>
                <nbspace/>
                <ITEXT CH="!) peut être converti en un jet à 4g2, représentant le plus fort «"/>
                <nbspace/>
                <ITEXT CH="potentiel"/>
                <nbspace/>
                <ITEXT CH="» du personnage."/>
                <para PARENT="Petit texte"/>
            </StoryText>
            <CSTOP RAMP="0" NAME="Libre Yellow 3" SHADE="100" TRANS="0.05"/>
