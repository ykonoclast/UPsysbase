        <PAGEOBJECT XPOS="173.701417322835" YPOS="8070.39212598426" OwnPage="17" ItemID="687514863" PTYPE="4" WIDTH="479.055118110236" HEIGHT="42.5196850393659" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="1" COLGAP="0" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L479.055 0 L479.055 42.5197 L0 42.5197 L0 0 Z" copath="M0 0 L479.055 0 L479.055 42.5197 L0 42.5197 L0 0 Z" gXpos="173.701417322835" gYpos="2779.05543307087" gWidth="0" gHeight="0" PSTYLE="Intro chapitre" LAYER="1" NEXTITEM="-1" BACKITEM="-1">
            <StoryText>
                <DefaultStyle PARENT="Intro chapitre"/>
                <ITEXT FONT="P052 Bold Italic" FONTSIZE="11" FCOLOR="Libre Yellow 1" CH="Ce chapitre aborde les principales mécaniques de résolution des actions dans le UP!System. Dans la mesure où la plupart de celles-ci peuvent être modélisées par un jet de compétence, c’est ce type de jet, véritable cœur du système, qui se taillera la part du lion en son sein."/>
                <trail PARENT="Intro chapitre"/>
            </StoryText>
        </PAGEOBJECT>
        <PAGEOBJECT XPOS="312.59905511811" YPOS="8112.91181102363" OwnPage="17" ItemID="687564351" PTYPE="4" WIDTH="340.157480314961" HEIGHT="608.031496062994" FRTYPE="0" CLIPEDIT="0" PWIDTH="1" PLINEART="1" LOCALSCX="1" LOCALSCY="1" LOCALX="0" LOCALY="0" LOCALROT="0" PICART="1" SCALETYPE="1" RATIO="1" COLUMNS="2" COLGAP="14.1732283464567" AUTOTEXT="0" EXTRA="0" TEXTRA="0" BEXTRA="0" REXTRA="0" VAlign="0" FLOP="1" PLTSHOW="0" BASEOF="0" textPathType="0" textPathFlipped="0" path="M0 0 L340.157 0 L340.157 608.031 L0 608.031 L0 0 Z" copath="M0 0 L340.157 0 L340.157 608.031 L0 608.031 L0 0 Z" gXpos="312.59905511811" gYpos="2821.57511811024" gWidth="0" gHeight="0" ALIGN="3" LAYER="1" NEXTITEM="652613471" BACKITEM="-1">
            <StoryText>
                <DefaultStyle ALIGN="3" LINESPMode="1" CPARENT="Default Character Style"/>
                <ITEXT CH="Jet de compétence"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="En UP!System la résolution des actions standards obéit à une série de stades simples appelée "/>
                <ITEXT CPARENT="Corps italique" CH="jet de compétence"/>
                <ITEXT CH=" et correspondant aux étapes suivantes"/>
                <nbspace/>
                <ITEXT CH=":"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="1 -"/>
                <ITEXT CH=" Le MJ fixe le "/>
                <ITEXT CPARENT="Corps italique" CH="Niveau de Difficulté"/>
                <ITEXT CH=" (ND) de l'action. Ce score devra être "/>
                <ITEXT CPARENT="Corps souligné" CH="dépassé"/>
                <ITEXT CH=" par le joueur dans son jet de compétence pour que l'action soit réussie."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="2 -"/>
                <ITEXT CH=" Le MJ annonce au joueur quels "/>
                <ITEXT CPARENT="Corps souligné" CH="trait"/>
                <ITEXT CH=" et "/>
                <ITEXT CPARENT="Corps souligné" CH="compétence"/>
                <ITEXT CH=" sont concernés par le jet puis, s'il le désire, le niveau de difficulté du jet (qui peut donc rester secret)."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="3 -"/>
                <ITEXT CH=" Le joueur fait la "/>
                <ITEXT CPARENT="Corps souligné" CH="somme"/>
                <ITEXT CH=" des rangs du "/>
                <ITEXT CPARENT="Corps souligné" CH="domaine"/>
                <ITEXT CH=" auquel la compétence est liée et de la "/>
                <ITEXT CPARENT="Corps souligné" CH="compétence"/>
                <ITEXT CH=" elle-même, ceci constitue son "/>
                <ITEXT CPARENT="Corps italique" CH="nombre de dés lancés"/>
                <ITEXT CH="."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="4 -"/>
                <ITEXT CH=" Le joueur établit son "/>
                <ITEXT CPARENT="Corps italique" CH="nombre de dés gardés"/>
                <ITEXT CH=" : celui-ci est égal au rang du "/>
                <ITEXT CPARENT="Corps souligné" CH="trait"/>
                <ITEXT CH=" employé. "/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="5 -"/>
                <ITEXT CH=" Le joueur "/>
                <ITEXT CPARENT="Corps souligné" CH="lance"/>
                <ITEXT CH=" son nombre de dés lancés."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="6 -"/>
                <ITEXT CH=" Parmi les dés lancés, le joueur "/>
                <ITEXT CPARENT="Corps souligné" CH="garde"/>
                <ITEXT CH=" autant de dés (les plus élevés) que son nombre de dés gardés et "/>
                <ITEXT CPARENT="Corps souligné" CH="écarte"/>
                <ITEXT CH=" les autres."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="7 - "/>
                <ITEXT CH="Le joueur effectue la "/>
                <ITEXT CPARENT="Corps souligné" CH="somme"/>
                <ITEXT CH=" de ses dés gardés et annonce le résultat au MJ qui interprète alors l'issue de l'action."/>
                <para PARENT="Puces corps"/>
                <ITEXT CH="Niveaux de difficulté"/>
                <para PARENT="Titre section"/>
                <ITEXT CPARENT="Corps gras" CH="ND10 :"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Corps italique" CH="facile :"/>
                <ITEXT CH=" c'est la routine du domaine considéré. Un novice est déjà un peu perdu mais, à partir de l'apprenti, tout le monde est capable d'accomplir cette tâche "/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemples : connaître les couleurs des familles majeures du royaume, passer dans une pièce sans réveiller une personne endormie."/>
                <para PARENT="Exemples"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="ND15 : "/>
                <ITEXT CPARENT="Corps italique" CH="moyen :"/>
                <ITEXT CH=" une tâche de difficulté normale pour le domaine considéré. C'est quelque chose qu'un professionnel parviendra presque systématiquement à faire mais qu'un apprenti aura quand même des chances réelles de rater."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemples : forger un fer à cheval, connaître les formes d'adresse standard de la cour et les salutations d'usage, se dissimuler dans les ombres, monter un cheval au galop."/>
                <para PARENT="Exemples"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="ND25 :"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Corps italique" CH="difficile :"/>
                <ITEXT CH=" c'est un travail délicat qui sort de ce que l'on demande habituellement dans le domaine. Un professionnel commence à devoir considérer les chances d'échec, un expert y parvient sans faillir, mais un apprenti est en réelle difficulté."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemples : se battre en équilibre sur une poutre, connaître toutes les subtilités de l'étiquette, poignarder un homme dans un bal masqué et sortir de la salle sans être remarqué, faire de gracieuses acrobaties martiales accroché à un lustre."/>
                <para PARENT="Exemples"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="ND40 :"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Corps italique" CH="très difficile :"/>
                <ITEXT CH=" quelque chose qui sort vraiment de l'ordinaire et dont même un professionnel a peu de chance de se sortir. Un expert n'est pas certain de l'issue, même s'il n'est pas trop en danger. Seul un maître affronte une telle difficulté complètement serein."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemples : d'un coin de la pièce deviner la teneur d'une conversation entre deux personnes plus loin à un bal, discerner le rang social d'une personne à son entrée dans la pièce, planter un carreau d'arbalète dans une pomme sur la tête de son fils, se dissimuler dans une pièce normale éclairée."/>
                <para PARENT="Exemples"/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Exemple de jet complet : Éleuthéria, est amenée devant l'empereur de Ganae, Solos le jeune. Elle doit se prosterner correctement. Tout crime de lèse-majesté pourrait être puni de mort par le capricieux jeune monarque."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Le MJ estime que l’action est difficile et fixe un ND de 25. Il demande au joueur incarnant Éleuthéria d'effectuer un jet d'Étiquette/Mental."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Éleuthéria possède, un mental de 3, un total de 3 rangs dans le domaine Social dont dépend la compétence Étiquette et un rang 3 en étiquette."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Le joueur lance donc 6 dés (Social + Étiquette) et obtient :"/>
                <para PARENT="Exemples"/>
                <ITEXT CH="10"/>
                <nbspace/>
                <ITEXT CH="; 7"/>
                <nbspace/>
                <ITEXT CH="; 6"/>
                <nbspace/>
                <ITEXT CH="; 6"/>
                <nbspace/>
                <ITEXT CH="; 5 et 4."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Il va maintenant garder les 3 plus élevés (car le Mental est de 3) et écarter les autres, il reste"/>
                <nbspace/>
                <ITEXT CH=":"/>
                <para PARENT="Exemples"/>
                <ITEXT CH="10"/>
                <nbspace/>
                <ITEXT CH=";"/>
                <nbspace/>
                <ITEXT CH="7"/>
                <nbspace/>
                <ITEXT CH=";"/>
                <nbspace/>
                <ITEXT CH="6."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Soit un total de 23. Ce n'est pas assez pour réussir l'action dont le ND est à 25"/>
                <nbspace/>
                <ITEXT CH="! Mais un point de règle non révélé peut encore sauver Éleuthéria..."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Dé explosif"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="Si un dé fait un "/>
                <ITEXT CPARENT="Corps souligné" CH="10"/>
                <ITEXT CH=", il est "/>
                <ITEXT CPARENT="Corps souligné" CH="relancé"/>
                <ITEXT CH=" et son nouveau résultat est "/>
                <ITEXT CPARENT="Corps souligné" CH="ajouté"/>
                <ITEXT CH=" au précédent. On dit que le dé "/>
                <ITEXT CPARENT="Corps italique" CH="explose"/>
                <ITEXT CH=". Il compte toujours pour "/>
                <ITEXT CPARENT="Corps souligné" CH="un seul dé"/>
                <ITEXT CH=" pour ce qui est de les garder."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemple"/>
                <nbspace/>
                <ITEXT CH=": le joueur incarnant Éleuthéria relance le 10 et obtient un 2, le total pour ce dé valant désormais 12, au final il a donc les résultats suivants"/>
                <nbspace/>
                <ITEXT CH=":"/>
                <para PARENT="Exemples"/>
                <ITEXT CH="12"/>
                <nbspace/>
                <ITEXT CH="; 7"/>
                <nbspace/>
                <ITEXT CH="; 6."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Soit un total de 25"/>
                <nbspace/>
                <ITEXT CH=": c'est réussi, mais à quel point"/>
                <nbspace/>
                <ITEXT CH="? Observons le point de règle suivant pour le savoir."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Incréments de réussite"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="On quantifie le degré de réussite d'une action en "/>
                <ITEXT CPARENT="Corps italique" CH="incréments de réussite"/>
                <ITEXT CH=". Un incrément est une "/>
                <ITEXT CPARENT="Corps souligné" CH="marge de 5"/>
                <ITEXT CH=" au dessus du ND."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Il y a quatre degrés de réussite à une action"/>
                <nbspace/>
                <ITEXT CH=":"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="0 incrément (de ND à ND+4)"/>
                <nbspace CPARENT="Corps gras"/>
                <ITEXT CPARENT="Corps gras" CH=":"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Corps italique" CH="réussite sur le fil"/>
                <nbspace CPARENT="Corps italique"/>
                <ITEXT CPARENT="Corps italique" CH=":"/>
                <ITEXT CH=" peut-être le personnage s'est il tapé sur le pouce en plantant un clou. Toujours est-il que, malgré quelques péripéties, la tâche est accomplie. À noter que ceci est assez différent de la réussite telle qu'habituellement présentée dans la plupart des autres systèmes."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="1 incrément (de ND+5 à ND+9)"/>
                <nbspace CPARENT="Corps gras"/>
                <ITEXT CPARENT="Corps gras" CH=":"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Corps italique" CH="bonne réussite"/>
                <nbspace CPARENT="Corps italique"/>
                <ITEXT CPARENT="Corps italique" CH=":"/>
                <ITEXT CH=" il n'y a pas de quoi être particulièrement fier mais un certain contentement est justifié. La joie du travail accompli."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="2 incréments (de ND+10 à ND+14 )"/>
                <nbspace CPARENT="Corps gras"/>
                <ITEXT CPARENT="Corps gras" CH=":"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Corps italique" CH="très bonne réussite"/>
                <nbspace CPARENT="Corps italique"/>
                <ITEXT CPARENT="Corps italique" CH=":"/>
                <ITEXT CH=" c'est vraiment du très bon travail, de quoi être fier de soi."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="3+ incréments (à partir de ND+15)"/>
                <nbspace CPARENT="Corps gras"/>
                <ITEXT CPARENT="Corps gras" CH=":"/>
                <ITEXT CH=" "/>
                <ITEXT CPARENT="Corps italique" CH="réussite exceptionnelle"/>
                <nbspace CPARENT="Corps italique"/>
                <ITEXT CPARENT="Corps italique" CH=":"/>
                <ITEXT CH=" on ne peut rien reprocher à l'action. Attention, ce n'est cependant pas une «"/>
                <nbspace/>
                <ITEXT CH="réussite critique"/>
                <nbspace/>
                <ITEXT CH="» au sens habituel rôlistique du terme. Il n'y pas vraiment de critiques (positifs comme négatifs) dans le UP! System."/>
                <para PARENT="Puces corps"/>
                <breakcol/>
                <ITEXT CH="Exemple : avec son 25, Éleuthéria a donc réussi sur le fil"/>
                <nbspace/>
                <ITEXT CH=": elle n'a aucun incrément! Peut-être a-t-elle légèrement trébuché ou bafouillé en marmonnant les salutations cérémonielles. Cela passe à peu près mais quelques sourires seront esquissés dans la noble assemblée. Mais qui sait, peut être un nouveau point de règle va-t-il l'aider à mieux réussir"/>
                <nbspace/>
                <ITEXT CH="?"/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Sources de bonus"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="Des bonus peuvent être accordés à certaines valeurs chiffrées d’un personnage (caractéristiques, jets etc.). Ces bonus peuvent provenir des circonstances (le MJ estime par exemple que, dans le cadre d’un jet social, les arguments avancés par le joueur en roleplay sont particulièrement convaincants), de l’équipement (un viseur sur une arme) ou d’autres forces (comme un pouvoir particulier)."/>
                <para PARENT="Corps de texte"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="Bonus à une valeur calculée"/>
                <nbspace CPARENT="Corps gras"/>
                <ITEXT CPARENT="Corps gras" CH=": "/>
                <ITEXT CH="il arrive que l'on calcule une valeur à partir d'une caractéristique. La défense en est un bon exemple (elle est calculée à partir de la Coordination). Cela peut aussi arriver dans d’autres cas comme le calcul d’un ND (on pourrait baser le ND d’un jet visant à tromper sur le Mental de la cible par exemple)."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Tout bonus à une valeur calculée ne s'applique pas "/>
                <ITEXT CPARENT="Corps souligné" CH="aux jets"/>
                <ITEXT CH=" effectués avec la ou les caractéristiques entrant dans son calcul."/>
                <para PARENT="Corps de texte"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="Bonus aux jets"/>
                <nbspace CPARENT="Corps gras"/>
                <ITEXT CPARENT="Corps gras" CH=":"/>
                <ITEXT CH=" un tel bonus est "/>
                <ITEXT CPARENT="Corps souligné" CH="ajouté"/>
                <ITEXT CH=" au score obtenu aux dés. Ce type de bonus ne compte que pour un jet spécifique. Il ne peut "/>
                <ITEXT CPARENT="Corps souligné" CH="pas"/>
                <ITEXT CH=" être ajouté à une valeur calculée à partir d'une ou plusieurs des caractéristiques utilisées pour le jet."/>
                <para PARENT="Corps de texte"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="Bonus de rang"/>
                <nbspace CPARENT="Corps gras"/>
                <ITEXT CPARENT="Corps gras" CH=":"/>
                <ITEXT CH="  lorsqu'un personnage atteint le "/>
                <ITEXT CPARENT="Corps souligné" CH="rang 3"/>
                <ITEXT CH=" dans une de ses compétences (niveau professionnel), il obtient un "/>
                <ITEXT CPARENT="Corps souligné" CH="bonus permanent de 5"/>
                <ITEXT CH=" à tous les jets effectués avec cette compétence. Ce bonus représente la marge existant entre un professionnel et tout niveau inférieur."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Lorsqu'un personnage atteint le "/>
                <ITEXT CPARENT="Corps souligné" CH="rang 5"/>
                <ITEXT CH=" (maître), il peut, de plus, "/>
                <ITEXT CPARENT="Corps souligné" CH="relancer une fois"/>
                <ITEXT CH=" tout jet de cette compétence en dépensant un point de force d'âme (cf. plus loin)."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemple"/>
                <nbspace/>
                <ITEXT CH=": cela tombe bien, Éleuthéria a un rang 3 dans sa compétence Étiquette! Elle obtient donc un bonus de 5 à tous ses jets avec cette compétence. Le résultat précédent passe donc de 25 à 30."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="C'est désormais 5 points au dessus du ND"/>
                <nbspace/>
                <ITEXT CH=": l’action est donc réussie avec un incrément, rien ne peut vraiment lui être reproché."/>
                <para PARENT="Exemples"/>
                <breakframe/>
                <ITEXT CH="À ses côtés, son père, le grand chambellan Éleutherios (prenons un instant pour saluer la constance familiale dans le nommage des enfants) se prosterne lui aussi. Le grand chambellan possède un Mental de 4, un domaine Social de 5 et une compétence Étiquette de 5. C'est un redoutable courtisan : il lance dix dés et en garde 4. Mais même les plus grands ont leurs faux pas et Éleutherios n'obtient qu'un résultat de 17 à son jet. C'est d'autant plus grave qu'on attend de lui la perfection"/>
                <nbspace/>
                <ITEXT CH="!"/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Heureusement, étant un maître (rang 5) de l'Étiquette, il a la possibilité de dépenser un point de force d'âme pour relancer son jet"/>
                <nbspace/>
                <ITEXT CH=": il le fait et obtient 31, c'est déjà une bonne réussite. Il y ajoute son bonus de 5 (car un rang 5 dans une compétence signifie que l'on a aussi un rang 3) et obtient un résultat final de 36, soit deux incréments par rapport au ND de l'action (difficile"/>
                <nbspace/>
                <ITEXT CH=": 25). C'est une très bonne réussite"/>
                <nbspace/>
                <ITEXT CH=": on n'en attendait pas moins de maître Éleutherios"/>
                <nbspace/>
                <ITEXT CH="!"/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Heureusement pour lui qu'Éleutherios n'a pas échoué une seconde fois car l'on ne peut relancer qu'une fois un jet en dépensant de la force d'âme. Il aurait alors perdu un point de force d'âme pour rien."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Emploi des spécialisations"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="Lorsqu'un personnage possède une spécialisation dans  la compétence utilisée en rapport avec l'action entreprise, il peut "/>
                <ITEXT CPARENT="Corps souligné" CH="ajouter un dé"/>
                <ITEXT CH=" à son nombre de dés lancés."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemple : quittant la cour avec son père, Éleuthéria ose demander à celui-ci ce qu’il a pensé de sa prestation. Il répond qu’il l’a trouvée tout juste passable. Le joueur d’Éleuthéria suspecte ce PNJ de lui dissimuler ses pensées réelles. Il demande donc à effectuer un jet d’Empathie."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Éleuthéria ne possède qu’un rang 2 en Empathie, ce qui, additionné à ses trois rangs dans le domaine Analyse, lui conférerait normalement cinq dés lancés. Mais une vie passée dans les faux semblants du Grand Palais lui a appris à discerner le vrai du faux, justifiant sa spécialisation «"/>
                <nbspace/>
                <ITEXT CH="mensonges"/>
                <nbspace/>
                <ITEXT CH="» dans cette compétence. Le MJ considérant que le jet est effectué dans le cadre de cette spécialisation, un dé supplémentaire peut être ajouté au nombre de dés lancés, portant ce total à six."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Éleuthéria ne bénéficie par du bonus de 5 accordé pour un rang 3 de compétence"/>
                <nbspace/>
                <ITEXT CH=": la spécialisation apporte un dé lancé mais pas un véritable rang de compétence."/>
                <para PARENT="Exemples"/>
                <ITEXT CH="Force d'âme"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="La "/>
                <ITEXT CPARENT="Corps italique" CH="force d’âme"/>
                <ITEXT CH=" représente la capacité d’un personnage à se dépasser. Elle peut représenter de l’aplomb, des «"/>
                <nbspace/>
                <ITEXT CH="tripes"/>
                <nbspace/>
                <ITEXT CH="» ou quelque chose de plus ésotérique, comme une sorte de réserve d’énergie mystique.  «"/>
                <nbspace/>
                <ITEXT CH="Force d’âme"/>
                <nbspace/>
                <ITEXT CH="» est le terme générique du UP!System de base, les différents jeux qui y sont adaptés peuvent être amenés à changer le nom de cette caractéristique pour mieux l'inscrire dans leur contexte spécifique (c’est d’ailleurs le cas de tous les suppléments UP!System)."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="La force d’âme est une jauge, les règles afférentes au nombre de points de force d’âme disponibles ainsi qu’aux modalités de sa récupération sont détaillées dans les chapitres «"/>
                <nbspace/>
                <ITEXT CH="caractéristiques"/>
                <nbspace/>
                <ITEXT CH="» et «"/>
                <nbspace/>
                <ITEXT CH="état vital"/>
                <nbspace/>
                <ITEXT CH="»."/>
                <para PARENT="Corps de texte"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="Utilisation : "/>
                <ITEXT CH="nombre de règles font référence à la dépense de points de force d’âme (comme le bonus de rang 5 dans une compétence, mais aussi bien des pouvoirs spécifiques). En plus de celles-ci, l’utilisation de base de la force d’âme est l’obtention de "/>
                <ITEXT CPARENT="Corps souligné" CH="dés bonus"/>
                <nbspace/>
                <ITEXT CH=": chaque point dépensé sur un jet y ajoute un dé "/>
                <ITEXT CPARENT="Corps souligné" CH="lancé et gardé"/>
                <ITEXT CH=". Cette dépense peut être effectuée même après le jet (pour améliorer un résultat après coup). Ces dés explosent normalement."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemple : admettons que, malgré sa spécialisation, Éleuthéria ne soit pas parvenue à discerner le vrai du faux dans la réponse de son géniteur, obtenant par exemple 21 pour un ND de 25 (qui pourrait représenter le résultat d’un jet de Subterfuge d’Éleuthérios). Son joueur décide alors de dépenser un point de force d’âme pour ajouter un dé lancé et gardé à son jet. Le dé fait un 10 qui explose normalement pour un total de 13, portant le score final à 34 (un incrément par rapport à 25)"/>
                <nbspace/>
                <ITEXT CH=": Éleuthéria discerne que, derrière son habituelle rudesse, le grand chambellan dissimule sa fierté pour sa fille."/>
                <para PARENT="Exemples"/>
                <breakframe/>
                <ITEXT CH="Jet de trait pur"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="On n’utilise les traits seuls que pour la résolution des actions les plus simples. Ce n’est pas la majorité des cas mais il peut arriver qu’une action ne puisse réellement être rattachée à une compétence tant elle s’appuie simplement sur les forces innées d’un personnage plutôt que sur ce qu’il a appris à faire."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="On lance et garde alors autant de dés que le trait."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Le ND pour ce type de jet est celui correspondant à la difficulté standard immédiatement inférieure."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemple : Un jet de Volonté difficile a pour ND 15 (correspondant à la difficulté moyenne standard)."/>
                <para PARENT="Exemples"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Voici des exemples de circonstances où l’on emploiera un jet de trait pur"/>
                <nbspace/>
                <ITEXT CH=":"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="Jet de perception de base (voir les ennemis) :"/>
                <ITEXT CH=" jet de mental."/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="Jet de connaissance de base (nom du roi, date d'indépendance de la région) : "/>
                <ITEXT CH="jet de mental"/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="Jet de résistance mentale (résister à la torture ou la peur, encaisser la perte d'un proche) :"/>
                <ITEXT CH=" jet de volonté"/>
                <para PARENT="Puces corps"/>
                <ITEXT CPARENT="Corps gras" CH="Jet de tour de force simple (enfoncer une porte, soulever un poids) :"/>
                <ITEXT CH=" jet de physique"/>
                <para PARENT="Puces corps"/>
                <ITEXT CH="Jets en opposition"/>
                <para PARENT="Titre section"/>
                <ITEXT CH="Il existe trois types de jets en opposition :"/>
                <para PARENT="Corps de texte"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="Jet contre adversaire actif :"/>
                <ITEXT CH=" lorsque l'adversaire s'oppose activement à l'action entreprise les deux parties effectuent un jet, le personnage résistant a pour ND le "/>
                <ITEXT CPARENT="Corps souligné" CH="résultat"/>
                <ITEXT CH=" du jet du personnage entreprenant l'action, s'l réussit il parvient à résister."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemple : un personnage tente de frapper un autre qui pare..."/>
                <para PARENT="Exemples"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="Actions symétriques en opposition :"/>
                <ITEXT CH=" on sera dans cette situation si deux actions symétriques s'opposent directement. Pour qu'un vainqueur émerge, il doit alors avoir au moins "/>
                <ITEXT CPARENT="Corps souligné" CH="un incrément"/>
                <ITEXT CH=" le séparant de son adversaire."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemple : Cnæus s'adonne au bras de fer avec un camarade de garnison, tous deux effectuent un jet de physique pur. Cnæus obtient 17 et son adversaire 15. Bien que Cnæus ait obtenu un résultat supérieur, ceci n'est pas suffisant car un incrément n'émerge pas de la différence. Le duel continuant, Cnæus obtient un 16 mais son adversaire s'effondre et ne parvient qu'à 10. Avec 6 points de score d'écart, Cnæus dispose d'au moins un incrément, il remporte le bras de fer."/>
                <para PARENT="Exemples"/>
                <para PARENT="Corps de texte"/>
                <ITEXT CPARENT="Corps gras" CH="Jet contre adversaire passif :"/>
                <ITEXT CH=" lorsque l'adversaire n'est pas en mesure d'agir réellement contre l'action, celle-ci s'effectue simplement avec la nécessité d'obtenir "/>
                <ITEXT CPARENT="Corps souligné" CH="autant d'incréments"/>
                <ITEXT CH=" que le "/>
                <ITEXT CPARENT="Corps souligné" CH="trait"/>
                <ITEXT CH=" que l'adversaire utilise pour résister. Par défaut le ND est moyen."/>
                <para PARENT="Corps de texte"/>
                <ITEXT CH="Exemple : utiliser un pouvoir mental (autant d'incréments que la volonté de l'adversaire) etc."/>
                <trail PARENT="Exemples"/>
            </StoryText>
        </PAGEOBJECT>
